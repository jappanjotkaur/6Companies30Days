class Solution {
  public:

    void matchPairs(int n, char nuts[], char bolts[]) {
        sort(nuts,nuts+n); //to sort the elements from the start of the array nuts[0] up to (but not including) nuts[n]
        for(int i=0;i<n;i++)
        {
            bolts[i]=nuts[i];
        }
    }
};
//TC:ð‘‚(ð‘›logð‘›)+ð‘‚(ð‘›)=ð‘‚(ð‘›logâ¡ð‘›)  Sorting Copying elements from nuts to bolts
//SC:O(1)
